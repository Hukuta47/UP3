<Page x:Class="IgoraSoftware.Pages.ByRole.Administrator.AdministratorMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IgoraSoftware.Pages.ByRole.Administrator"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdministratorMainPage">

    <Grid Margin="10">
		<StackPanel HorizontalAlignment="Center">
			<Button Content="Сформировать отчет по заказам"/>
			<Rectangle Height="10"/>
			<Button Content="История входов сотрудников" Click="HistoryLogins_Click"/>
			<Rectangle Height="10"/>
			<Button Content="Выйти с учетной записи" Click="ExitUser_Click" Background="#004433"/>
		</StackPanel>
	</Grid>
</Page>
